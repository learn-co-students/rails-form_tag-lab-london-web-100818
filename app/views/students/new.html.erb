 <%= form_tag students_path do %>
<label for="student_name">Student Form</label>
  <input id='first_name' type="text" name="student[first_name]">
  <input id='last_name' type="text" name="student[last_name]">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="submit" value="Submit Student">
  <%= puts params %>
<% end %>
